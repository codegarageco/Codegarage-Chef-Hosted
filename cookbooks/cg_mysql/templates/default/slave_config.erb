[mysqld]
sync_binlog = 1
read_only = 1
innodb_flush_log_at_trx_commit = 1
log_bin = /var/log/mysql-<%= @data[:service_name] %>/mysql-bin.log
log_error = /var/log/mysql-<%= @data[:service_name] %>/error.log
relay-log = /var/log/mysql-<%= @data[:service_name] %>/mysql-relay-bin.log
server-id = <%= @data[:server_id] %>
